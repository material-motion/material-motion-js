{
  "private": true,
  "name": "material-motion-demos-react",
  "version": "0.1.0",
  "scripts": {
    "pundle": "../../node_modules/.bin/pundle",
    "start": "yarn run pundle --dev",
    "clean": "rm -rf ./site/dist/*; mkdir -p ./site/dist/",
    "lint": "../../node_modules/.bin/tslint -c ../../tslint.json --project tsconfig.json --type-check",
    "build": "yarn run clean; yarn run pundle",
    "deploy": "yarn run build; ../../node_modules/.bin/firebase deploy"
  },
  "dependencies": {
    "jss": "9.2",
    "jss-preset-default": "4.0.1",
    "jsxstyle": "2",
    "material-motion": "^0.1.0",
    "react": "15.4.1",
    "react-dom": "15.4.1",
    "react-router": "4.0.0-alpha.6",
    "tslib": "^1.2.0"
  },
  "devDependencies": {
    "@types/react": "^15.0.0",
    "firebase-tools": "6.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:material-motion/material-motion-js.git"
  },
